{
  "build": {
    "builder": "nixpacks",
    "nixpacksPlan": {
      "providers": ["python"],
      "phases": {
        "build": {
          "cmds": ["cd src/components/on-call-coaching && pip install -r requirements.txt"]
        }
      }
    }
  },
  "deploy": {
    "startCommand": "cd src/components/on-call-coaching && python script.py"
  }
}